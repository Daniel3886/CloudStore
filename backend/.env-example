# ===============================
# Backend Configuration Example
# ===============================

# --- Database (Supabase) ---
# Replace with your Supabase database URL and credentials or whatever else you want to use
SPRING_DATASOURCE_URL=jdbc:postgresql://your-db-host:5432/your-db-name
SPRING_DATASOURCE_USERNAME=your_db_username
SPRING_DATASOURCE_PASSWORD=your_db_password

# --- JWT Authentication ---
# Secret key used for signing JWTs
JWT_SECRET_KEY=your_jwt_secret_key_here

# --- Amazon S3 Configuration ---
# AWS S3 bucket for file storage
AWS_BUCKET_NAME=your_bucket_name
AWS_ACCESS_KEY=your_aws_access_key
AWS_SECRET_KEY=your_aws_secret_key
AWS_REGION=your_aws_region

# --- Email Configuration ---
# Email account used to send verification codes to users
SUPPORT_EMAIL=your_email@example.com
APP_PASSWORD=your_email_app_password
spring.mail.properties.mail.debug=true
